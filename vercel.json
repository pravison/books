{
    "builds":[{
        "src": "accountant/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
    }],
    "routes": [{
        "src":"/(.*)",
        "dest": "accountant/wsgi.py"
    }],

    "rewrites": [
        {
            "source": "/:subdomain/:path*",
            "destination": "/accountant/wsgi.py"
        },
        {
            "source": "/(.*)",
            "destination": "/accountant/wsgi.py"
        }
    ]
}